#alias
alias app="cd /var/www/app"
alias ll="ls -alhF"
alias memflush="echo \"flush_all\" | nc servicememcached 11211"
alias xon="sed -i \"s/xdebug\.remote_enable=0/xdebug\.remote_enable=1/\" /usr/local/etc/php/conf.d/docker-php-ext-xdebug.ini && kill -USR2 1"
alias xoff="sed -i \"s/xdebug\.remote_enable=1/xdebug\.remote_enable=0/\" /usr/local/etc/php/conf.d/docker-php-ext-xdebug.ini && kill -USR2 1"